#!/bin/sh

export PATH="$PATH:/home/kstroz/.local/bin"

case "$(uname -s)" in
  Linux)
    eval "$(/home/linuxbrew/.linuxbrew/bin/brew shellenv)"
  ;;
  Darwin)
  ;;
esac

# fnm
eval "$(fnm env --use-on-cd)"

# pnpm
export PNPM_HOME="$XDG_DATA_HOME/pnpm"
case ":$PATH:" in
  *":$PNPM_HOME:"*) ;;
  *) export PATH="$PNPM_HOME:$PATH" ;;
esac
